            android:text="@string/descricao2"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/im_banda"
            app:layout_constraintTop_toBottomOf="@+id/nome_banda" />

